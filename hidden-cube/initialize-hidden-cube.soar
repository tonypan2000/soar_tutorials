sp {propose*initialize-find-hidden-cube
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-hidden-cube
        ^actions.move-lift <ml>
        ^actions.move-head <mh>)
   (<ml> ^height 0.0)
   (<mh> ^angle 0.0)
}

sp {apply*initialize-cube-stack
   (state <s> ^operator <op>)
   (<op> ^name initialize-hidden-cube)
-->
   (<s> ^name hidden-cube)
}

sp {cube-stack*propose*save-origin-pose
   (state <s> ^name hidden-cube
              ^io.input-link.pose <p>
             -^origin-pose)
   (<p> ^rot <rot>
        ^x <x-val>
        ^y <y-val>
        ^z <z-val>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name save-origin-pose
         ^pose <origin>)
   (<origin> ^rot <rot>
             ^x <x-val>
             ^y <y-val>
             ^z <z-val>)
}

sp {apply*save-origin-pose
   (state <s> ^operator <op>)
   (<op> ^name save-origin-pose
         ^pose <origin>)
   (<origin> ^rot <rot>
             ^x <x-val>
             ^y <y-val>
             ^z <z-val>)
-->
   (<s> ^origin-pose <o-pose>)
   (<o-pose> ^rot <rot>
             ^x <x-val>
             ^y <y-val>
             ^z <z-val>)
}

sp {cube-stack*compare*save-origin-pose
   (state <s> ^name hidden-cube
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name save-origin-pose)
   (<op2> ^name)
-->
   (<s> ^operator <op1> > <op2>)
}
