sp {propose*initialize-simple-bot-agent
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-simple-bot-agent)
}

sp {apply*initialize-simple-bot-agent
   (state <s> ^operator <op>)
   (<op> ^name initialize-simple-bot-agent)
-->
   (<s> ^name tanksoar)
}

